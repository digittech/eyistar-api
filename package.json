{
  "name": "digit",
  "version": "0.0.3",
  "description": "",
  "author": "Lala Ayoade <ayoade@yahoo.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "prestart": "npm run copy:assets",
    "start": "nest start",
    "prestart:dev": "npm run copy:assets",
    "start:dev": "nest start --watch",
    "prestart:debug": "npm run copy:assets",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run copy:assets",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "db:hydrate": "npx typeorm-model-generator",
    "db:diagram": "typeorm-uml ormconfig.json",
    "migrate:create": "typeorm migration:create -c migration -n ",
    "migrate:run": "typeorm migration:run -c migration",
    "migrate:revert": "typeorm migration:revert -c migration",
    "seed:create": "typeorm migration:create -c seed -n ",
    "seed:run": "typeorm migration:run -c seed",
    "seed:revert": "typeorm migration:revert -c seed",
    "subscriber:create": "typeorm subscriber:create -c migration -n ",
    "copy:assets": "cpx-fixed 'src/i18n/**' 'dist/i18n'"
  },
